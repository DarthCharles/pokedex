<h1>Pokemons#index</h1>
<p>Find me in app/views/pokemons/index.html.erb</p>

<div class="container" >
  <% @pokemon.each do |p| %>
  <table class="table table-striped table-bordered">
    <tr>
    <td class="span1"><%= p.nat %></td>
    <td class="span2"><%= p.name %></td>
    <td class="span2"><%= p.typeI %></td>
    <td class="span2"><%= p.typeII %></td>
    </tr>
  <% end %>
  </table>
</div>
